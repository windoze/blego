    "version": "0.2.0",
    "configurations": [
        {
            "name": "lpf2test",
            "type": "coreclr",
            "request": "launch",
            "program": "/usr/bin/dotnet",
            "args": ["bin/Debug/netcoreapp3.1/lpf2test.dll", "10"],
            "cwd": "~/repos/blego/lpf2test",
            "stopAtEntry": false,
            "console": "internalConsole",
            "pipeTransport": {
                "pipeCwd": "${workspaceRoot}",
                "pipeProgram": "/usr/bin/ssh",
                "pipeArgs": [
                    "u"
                ],
                "debuggerPath": "~/vsdbg/vsdbg"
            },
            "preLaunchTask": "deploy"
        },
        {
            "name": "bletest",
            "type": "coreclr",
            "request": "launch",
            "program": "/usr/bin/dotnet",
            "args": ["bin/Debug/netcoreapp3.1/bletest.dll", "10"],
            "cwd": "~/repos/blego/bletest",
            "stopAtEntry": false,
            "console": "internalConsole",
            "pipeTransport": {
                "pipeCwd": "${workspaceRoot}",
                "pipeProgram": "/usr/bin/ssh",
                "pipeArgs": [
                    "u"
                ],
                "debuggerPath": "~/vsdbg/vsdbg"
            },
            "preLaunchTask": "deploy"
        }
    ]
}